<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="433.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eg.edu.alexu.csd.datastructure.mailServer.GUIController">
   <children>
      <Button layoutX="192.0" layoutY="238.0" mnemonicParsing="false" onAction="#signin" prefHeight="26.0" prefWidth="128.0" text="Sign in" textFill="#008cff">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="signup1" layoutX="221.0" layoutY="306.0" mnemonicParsing="false" onAction="#signupInitializer" prefHeight="30.0" prefWidth="98.0" text="Sign up!" textFill="#e700ff">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="59.0" layoutY="311.0" text="No account? no problem">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="email" layoutX="140.0" layoutY="155.0" prefHeight="30.0" prefWidth="231.0" promptText="Email" />
      <PasswordField fx:id="password" layoutX="140.0" layoutY="192.0" prefHeight="30.0" prefWidth="231.0" promptText="Password" />
      <Label layoutX="192.0" layoutY="129.0" prefHeight="18.0" prefWidth="143.0" text="Enter your credentials" textFill="#008cff">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Label layoutX="31.0" layoutY="17.0" prefHeight="66.0" prefWidth="201.0" text="Mail server" textFill="#0045ff">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="32.0" />
         </font>
      </Label>
      <Label fx:id="successfulSignUp" layoutX="132.0" layoutY="68.0" prefHeight="30.0" prefWidth="276.0" textFill="#00971e">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="invalid" layoutX="136.0" layoutY="98.0" prefHeight="30.0" prefWidth="282.0" textFill="RED">
         <font>
            <Font name="Segoe UI Symbol" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
