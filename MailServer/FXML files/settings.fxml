<AnchorPane prefHeight="382.0" prefWidth="435.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eg.edu.alexu.csd.datastructure.mailServer.GUIController">
   <children>
      <Button layoutX="14.0" layoutY="125.0" mnemonicParsing="false" onAction="#changeName" text="Change my name!" textFill="#299a2b">
