<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="434.0" prefWidth="483.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eg.edu.alexu.csd.datastructure.mailServer.composeController">
   <children>
      <Label layoutX="28.0" layoutY="36.0" prefHeight="18.0" prefWidth="70.0" text="Sending to:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="num" layoutX="98.0" layoutY="32.0" mnemonicParsing="false" onAction="#num" prefHeight="26.0" prefWidth="70.0" text="10 users" textFill="#623fff">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextArea fx:id="body" layoutX="51.0" layoutY="110.0" prefHeight="198.0" prefWidth="383.0" promptText="Email body">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextArea>
      <TextField fx:id="topic" layoutX="51.0" layoutY="74.0" prefHeight="26.0" prefWidth="199.0" promptText="Topic">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <Button layoutX="318.0" layoutY="382.0" mnemonicParsing="false" onAction="#send" prefHeight="30.0" prefWidth="58.0" text="Send" textFill="#2ca724">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="387.0" layoutY="382.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" textFill="#ea3300">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <TextField fx:id="priority" layoutX="260.0" layoutY="74.0" prefHeight="30.0" prefWidth="175.0" promptText="Priority (Optional)">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <Button fx:id="browse" layoutX="151.0" layoutY="319.0" mnemonicParsing="false" onAction="#browse" text="Browse files" />
      <Label layoutX="44.0" layoutY="323.0" prefHeight="18.0" prefWidth="106.0" text="Add attachments:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="attnum" layoutX="243.0" layoutY="322.0" prefHeight="18.0" prefWidth="149.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="error" layoutX="181.0" layoutY="14.0" prefHeight="18.0" prefWidth="288.0" textFill="#fa0000">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="260.0" layoutY="50.0" prefHeight="18.0" prefWidth="162.0" text="*Between 1 and 10">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
